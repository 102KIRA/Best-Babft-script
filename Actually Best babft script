-- Original Code
local OriginalCode = [[loadstring(game:HttpGet("https://raw.githubusercontent.com/amd156935-create/BoatbuilderhubIsShitLexusHubIsBestAlso/refs/heads/main/9b21a6b954470aed.lua.txt"))()]]

-- Clipboard Function
local function copyToClipboard(text)
    if setclipboard then
        setclipboard(text)
    elseif syn and syn.set_clipboard then
        syn.set_clipboard(text)
    end
end

-- Button Click
CopyButton.MouseButton1Click:Connect(function()
    copyToClipboard(OriginalCode)
    CopyButton.Text = "COPIED!"
    task.wait(1.5)
    CopyButton.Text = "COPY ORGINAL CODE"
end)

